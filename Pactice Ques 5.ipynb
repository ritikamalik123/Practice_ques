    "def fact(x):\n",
    "    if x==1:\n",
    "        factorial=1\n",
    "    else:\n",
    "        factorial=(x-1)*fact(x)\n",
    "a=int(input(\"Enter any Number :-\"))\n",
    "prince=fact(a)\n",
    "print(\"factorial of the NUmber  :-\",prince)"
